{
  "expo": {
    "name": "InstaChat",
    "slug": "InstaChat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/chat.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/chat.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.khanna_ji.InstaChat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/chat.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/chat.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a70100bd-57e6-4a70-a55c-d33150ce4164"
      }
    }
  }
}
